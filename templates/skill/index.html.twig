{% extends 'base.html.twig' %}

{% block title %}Julien Delacourt - Compétences{% endblock %}

{% block content %}
<div class="row">
	<div class="col-lg-12 col-md-12 mx-auto">
       <h2>Compétences</h2>
		<div class="row">
			{% for skill in skills %}
				<div class="col-lg-1 text-center">
					<img  src="{{ asset('images/skills/' ~ skill.image) }}" alt="{{ skill.name }}"/>
					<br>
					{{ skill.image }}
				</div>
			{% else %}
				<em>no skill found</em>
			{% endfor %}

		</div>

	</div>
</div>
<hr>
{% endblock %}


