{% extends 'admin/dashboard.html.twig' %}
{% block title %}Ajouter une experience{% endblock %}
{% form_theme form 'bootstrap_4_layout.html.twig' %}
{% block header %}
<h2 class="pageheader-title">Dashboard - Ajouter une expérience</h2>
<div class="page-breadcrumb">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            {% block breadcrumb %}
            <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a></li>
            <li class="breadcrumb-item active" aria-current="page">Ajouter une expérience</li>
            {% endblock %}
        </ol>
    </nav>
</div>
{% endblock %}
{% block content %}
<div class="row">
    <div class="col-lg-12 col-md-12 mx-auto">
    	<h1>Ajouter une expérience</h1>
    	{{ form_start(form) }}
    	{{ form_row(form.title) }}
    	{{ form_row(form.entreprise) }}
    	{{ form_row(form.body) }}
    	{{ form_row(form.url) }}
    	{{ form_row(form.imageFile) }}
    	{{ form_row(form.year) }}
    	<button type="submit" class="btn btn-primary">Envoyer</button>
		<hr>
    	{{ form_end(form) }}

	</div>

</div>
{% endblock %}
